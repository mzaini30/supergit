{
  "name": "supergit",
  "version": "0.3.1",
  "description": "Running git push to multiple repositories automatically",
  "bin": {
    "s": "dist/status.mjs",
    "c": "dist/commit.mjs",
    "p": "dist/push.mjs"
  },
  "files": ["dist"],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "npm run build",
    "release": "bumpp --tag --commit --push",
    "prettier": "prettier --write \"**/*.ts\""
  },
  "simple-git-hooks": {
    "pre-push": "npm run prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzaini30/supergit.git"
  },
  "keywords": [
    "git",
    "automation"
  ],
  "author": "Zen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mzaini30/supergit/issues"
  },
  "type": "module",
  "homepage": "https://github.com/mzaini30/supergit#readme",
  "devDependencies": {
    "@types/node": "^17.0.42",
    "@types/readline-sync": "^1.4.4",
    "bumpp": "^7.1.1",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.11",
    "prettier": "^2.6.2",
    "readline-sync": "^1.4.10",
    "simple-git-hooks": "^2.8.0",
    "unbuild": "^0.7.4"
  }
}
